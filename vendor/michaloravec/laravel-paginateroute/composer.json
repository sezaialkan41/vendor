{
    "name": "michaloravec/laravel-paginateroute",
    "description": "Laravel outer extension to easily use laravel's paginator without the query string",
    "keywords": [
        "michaloravec",
        "laravel-paginateroute",
        "laravel",
        "pagination",
        "routing"
    ],
    "homepage": "https://github.com/michaloravec/laravel-paginateroute",
    "license": "MIT",
    "authors": [
        {
            "name": "Michal Oravec",
            "email": "michal.oravec@outlook.com",
            "homepage": "https://www.michaloravec.com",
            "role": "Developer"
        }
    ],
    "require": {
        "php" : ">=5.4.0"
    },
    "require-dev": {
        "phpunit/phpunit" : "^5.0",
        "illuminate/contracts": "~5.4.0",
        "illuminate/routing": "~5.4.0",
        "illuminate/translation": "~5.4.0",
        "orchestra/testbench": "^3.4"
    },
    "autoload": {
        "psr-4": {
            "MichalOravec\\PaginateRoute\\": "src"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "MichalOravec\\PaginateRoute\\Test\\": "tests"
        }
    },
    "extra": {
        "laravel": {
            "providers": [
                "MichalOravec\\PaginateRoute\\PaginateRouteServiceProvider"
            ],
            "aliases": {
                "PaginateRoute": "MichalOravec\\PaginateRoute\\PaginateRouteFacade"
            }
        }
    },
    "minimum-stability": "dev",
    "prefer-stable": true
}
